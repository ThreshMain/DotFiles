	HostkeyAlgorithms +ssh-rsa


Host *
	IdentityFile ~/.ssh/id_rsa
	VisualHostKey=yes
	HashKnownHosts yes
	ControlMaster auto
	ControlPath ~/.ssh/%r@%h:%p
	ServerAliveCountMax=30
	ServerAliveInterval=5
	AddKeysToAgent=yes
